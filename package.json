{
  "name": "angular2-webpack-starter",
  "version": "0.0.1",
  "scripts": {
    "clean:dist": "rimraf dist",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "webpack --config config/webpack.dev.js",
