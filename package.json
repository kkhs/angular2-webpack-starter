    "rimraf": "2.7.1",
    "rxjs": "5.5.12",
    "sass-loader": "4.1.1",
    "style-loader": "0.13.2",
    "ts-loader": "0.8.2",
    "typescript": "1.8.10",
    "typings": "1.5.0",
